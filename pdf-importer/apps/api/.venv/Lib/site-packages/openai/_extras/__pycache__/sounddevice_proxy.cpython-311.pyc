        private Version ParseVersion(string versionName)
        {
            if (string.IsNullOrWhiteSpace(versionName)) return new Version(0, 0);
            var cleaned = versionName.Trim().TrimStart('v', 'V');
            return Version.TryParse(cleaned, out var version) ? version : new Version(0, 0);
        }

        private async Task TriggerDownload(string url, string fileName)
        {
            await JS.InvokeVoidAsync("downloadFileFromUrl", url, fileName);
        }

        private List<string> NormalizeTerms(string input)
        {
            if (string.IsNullOrWhiteSpace(input)) return new();

            return input
                .ToLowerInvariant()
                .Replace("_", "")
                .Replace("-", "")
                .Split(' ', StringSplitOptions.RemoveEmptyEntries)
                .Select(term => term.Replace("v", "")) // optional "v" entfernen
                .ToList();
        }

        private string NormalizeString(string input)
        {
            return input
                .ToLowerInvariant()
                .Replace("_", "")
                .Replace("-", "")
                .Replace(" ", "")
                .Replace("v", "");
        }

